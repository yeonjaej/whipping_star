<?xml version="1.0" ?>

<mode name="nu" use="1"/>
<detector name="uBooNE" use="1" />

<channel name="nue" unit="MeV" use="1" numbins="16">
		<bins
			edges="100 200 300 400 500 600 700 800 900 1000 1200 1400 1600 1800 2000 2500 3000"
		/>
		<subchannel name="intrinsic" use="1" osc = "0"/>
		<subchannel name="leesignal" use="1" osc = "0"/>
</channel>

<channel name="numu" unit="MeV" use="1" numbins="16">
		<bins
			edges="100 200 300 400 500 600 700 800 900 1000 1200 1400 1600 1800 2000 2500 3000"
		/>
		<subchannel name="intrinsic" use="1" osc = "0"/>
</channel>

<channel name="ccpi0" unit="MeV" use="1" numbins="6">
		<bins
			edges="0 200 400 600 800 1000 2000"
		/>
		<subchannel name="intrinsic" use="1" osc = "0"/>
</channel>

<!-- ############################# MultiSims #####################################################-->
<MultisimFile treename="events" filename="/uboone/data/users/markross/SBNfit_example_data/1e1p.root" scale = "1.0" maxevents ="8000" >
	<parameters names = "ALL"/>
	<branch
		name="ereco"
		type="double"
		associated_hist="nu_uBooNE_nue_intrinsic"
	/>
</MultisimFile>

<MultisimFile treename="events" filename="/uboone/data/users/markross/SBNfit_example_data/signal.root" scale = "1.0" maxevents ="10000" >
	<parameters names = "ALL"/>
	<branch
		name="ereco"
		type="double"
		associated_hist="nu_uBooNE_nue_leesignal"
	/>
</MultisimFile>


<MultisimFile treename="events" filename="/uboone/data/users/markross/SBNfit_example_data/1m1p.root" scale = "1.0" maxevents ="20000" >
	<parameters names = "ALL"/>
	<branch
		name="ereco"
		type="double"
		associated_hist="nu_uBooNE_numu_intrinsic"
	/>
</MultisimFile>

<MultisimFile treename="events" filename="/uboone/data/users/markross/SBNfit_example_data/ccpi0.root" scale = "1.0" maxevents ="10000" >
	<parameters names = "ALL"/>
	<branch
		name="ereco"
		type="double"
		associated_hist="nu_uBooNE_ccpi0_intrinsic"
	/>
</MultisimFile>


<data path="" />
<covariance file="" name="" />
